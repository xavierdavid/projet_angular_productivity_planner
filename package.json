{
  "name": "productivity-planner",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:production": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:development": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged --verbose",
    "lint": "ng lint",
    "deploy:production": "npm run build:production && firebase use production && firebase deploy",
    "deploy:staging": "npm run build:staging && firebase use staging && firebase deploy",
    "deploy:development": "npm run build:development && firebase use development && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.17",
    "@angular/common": "^19.2.17",
    "@angular/compiler": "^19.2.17",
    "@angular/core": "^19.2.17",
    "@angular/forms": "^19.2.17",
    "@angular/platform-browser": "^19.2.17",
    "@angular/platform-browser-dynamic": "^19.2.17",
    "@angular/router": "^19.2.17",
    "@babel/runtime": "^7.26.10",
    "@ngrx/signals": "^19.0.1",
    "@npmcli/git": "^6.0.3",
    "@npmcli/package-json": "^6.1.1",
    "@npmcli/redact": "^3.1.1",
    "@npmcli/run-script": "^9.1.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "minipass-fetch": "^4.0.1",
    "nopt": "^8.1.0",
    "npm-package-arg": "^12.0.2",
    "rxjs": "^7.8.2",
    "semver": "^7.7.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular/cli": "^19.2.17",
    "@angular/compiler-cli": "^19.2.17",
    "@types/jest": "^29.5.14",
    "angular-eslint": "19.0.1",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "8.17.0",
    "vite": "^6.2.6"
  },
  "overrides": {
    "vite": "^6.2.6"
  }
